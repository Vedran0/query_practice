<% unless @bad_query %>
  $(".solution-<%= @solution.id %>").html('<%= j render "solutions/result", objects: @objects %>');
  $(".solution-time-<%= @solution.id %>").html('<%= j render "solutions/time", time: @time %>');
<% else %>
  $(".solution-<%= @solution.id %>").html('<div class="alert alert-danger"><%= @bad_query %></div>');
<% end %>