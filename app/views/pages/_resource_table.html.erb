<table class="table table-striped">
  <thead>
    <tr>
      <% @objects.attribute_names.each do |attribute| %>
        <th><%= attribute %></th>
      <% end %>
    </tr>
  </thead>

  <tbody>
    <% @objects.each do |object| %>
      <tr>
        <% @objects.attribute_names.each do |attribute| %>
          <td><%= eval("object.#{attribute}") %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>